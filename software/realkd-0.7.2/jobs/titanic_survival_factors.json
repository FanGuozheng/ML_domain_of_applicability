{
  "type" : "productWorkScheme",
  "id" : "titanic_survival_factors",
  "workspaces" : [ {
    "type" : "workspaceFromXarf",
    "id" : "titanic",
    "datafile" : "titanic_1.0.0.xarf"
  } ],
  "computations" : [ {
    "type" : "functionalDependencyComputation",
    "id" : "survival_gender_dependency",
    "relation" : {
      "type" : "binaryAttributeSetRelation",
      "table" : "titanic",
      "domain" : [ "sex" ],
      "codomain" : [ "survived" ]
    }
  }, {
    "type" : "legacyComputation",
    "id" : "subgroup_analysis",
    "algorithm" : "EXCEPTIONAL_SUBGROUP_BESTFIRST_BRANCHANDBOUND",
    "parameters" : {
      "modeling" : "bernoulli",
      "dev_measure" : "pos_prob_shift",
      "cov_weight" : "0.5",
      "attr_filter" : "[]",
      "max_depth" : "Optional[6]",
      "num_res" : "3",
      "targets" : "[survived]",
      "pos_category" : "1"
    }
  }, {
    "type" : "legacyComputation",
    "id" : "titanic_functional_pattern_discovery",
    "algorithm" : "FUNCTIONAL_PATTERN_DISCOVERY_OPUS",
    "parameters" : {
      "apx_fac" : "1",
      "oest" : "CHAIN",
      "num_res" : "3",
      "search_order" : "BREADTHFSPOTENTIAL",
      "targetAttributeParameter" : "[survived]"
    }
  }, {
    "type" : "exceptionalSubgroupComputation",
    "id" : "class_conditioned_gender_effect",
    "subgroup" : {
      "type" : "subgroup",
      "table" : "titanic",
      "selector" : {
        "type" : "attributeBasedConjunction",
        "table" : "titanic",
        "attributes" : [ "sex" ],
        "constraints" : [ {
          "type" : "equals",
          "value" : "female"
        } ]
      },
      "reference" : "global",
      "targetAttributes" : [ "class", "survived" ],
      "targetModellingMethod" : {
        "type" : "asDiscretlyConditionedBernoulli",
        "positiveCategory" : "1"
      }
    },
    "measure" : "reliableCondEffect"
  } ],
  "computationTimeLimit" : 3600
}